{
  "name": "blog-api",
  "version": "0.0.1",
  "description": "A sample blog api, demostrating frontend authentication",
  "author": "Oreofe Olurin <oreofeolurin@gmail.com>",
  "private": true,
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "postinstall": "npm run build",
    "build": "tsc -p tsconfig.json",
    "start:dev": "nodemon",
    "start": "node dist/main.js",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "engines": {
    "node": "12.x"
  },
  "dependencies": {
    "@nestjs/common": "^6.1.1",
    "@nestjs/core": "^6.1.1",
    "@nestjs/platform-express": "^6.1.1",
    "@nestjs/swagger": "^3.0.2",
    "@nestjs/websockets": "^6.1.1",
    "body-parser": "1.18.2",
    "class-transformer": "^0.1.9",
    "class-validator": "^0.9.1",
    "cors": "2.8.4",
    "express": "^4.16.3",
    "express-pino-logger": "2.0.0",
    "joi": "^13.6.0",
    "json-pick-keys": "0.0.4",
    "jsonwebtoken": "8.0.1",
    "reflect-metadata": "^0.1.12",
    "request": "^2.87.0",
    "request-promise-native": "1.0.5",
    "rxjs": "^6.0.0",
    "swagger-ui-express": "^4.0.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.0.0",
    "@types/body-parser": "1.16.5",
    "@types/express": "^4.0.39",
    "@types/jest": "^21.1.8",
    "@types/node": "^9.3.0",
    "@types/supertest": "^2.0.4",
    "dotenv": "^6.0.0",
    "jest": "^21.2.1",
    "nodemon": "^1.14.1",
    "prettier": "^1.11.1",
    "supertest": "^3.0.0",
    "ts-jest": "^21.2.4",
    "ts-loader": "^4.1.0",
    "ts-node": "^4.1.0",
    "tsconfig-paths": "^3.1.1",
    "tslint": "5.3.2",
    "typescript": "^3.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  }
}
